// Функция для удаления и возврата последнего элемента из стека
func pop() int {
}
func main() {
	// Пример использования стека для операций
	push(5)
	push(3)
	result := pop() + pop()
	push(result)
	// Вывод результата выполнения программы
	fmt.Println(stack[0]) // 8
}

